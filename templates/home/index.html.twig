{% block body %}
	<div class="container-fluid p-5">
		<div class="row">
			<div class="col-lg-4">
				<div class="pb-5">
					<img class="ps-5" src="{{ asset('build/images/title_home.svg') }}" alt="Title for home page">
				</div>
				<div class="d-flex flex-wrap">
					<a href="/agency"><img src="{{ asset('build/images/button_myagency.svg') }}" alt="button" class="m-3"></a>
